{
    "manifest_version": 2,
    "name": "Malendar - A New Tab page for Malayalees",
    "version": "2.1.0",
    "description": "Malendar is a simple new tab page with a malayalam calendar widget made by a Malayalee for all Malayalees",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "bookmarks",
        "management",
        "chrome://favicon/"
    ],
    "optional_permissions": [ "identity", "background", "alarms"],
    "author": "Ajay Balachandran",
    "icons": {
        "128": "icon128.png"
    },
    "oauth2": {
        "client_id": "347269516750-er172lb1qmphnci4r5l5md676v6jeb2d.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/calendar.readonly"
        ]
    },
    "key": "",

    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },

    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}